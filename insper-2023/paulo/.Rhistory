library(tidyverse)
library(sf)
sp<-sf::read_sf('LAYER_DISTRITO')
ggplot(sp) + geom_sf()
library(geojsonsf)
sp_geo <- sf_geojson(sp, simplify = TRUE, digits = 6)
jsonlite::write_json(sp_geo, 'dados2.json')
write(sp_geo, 'dados2.json')
sf::st_crs(sp)
write_file(sp_geo, 'dados2.json')
new_sp <- rmapshaper::ms_simplify(sp)
ggplot(new_sp) + geom_sf()
sp_geo <- sf_geojson(new_sp, simplify = TRUE, digits = 6)
sf::st_crs(sp)
write_file(sp_geo, 'dados2.json')
sf::st_crs(new_sp)
sp_wgs84 <- sf::st_transform(new_sp, 4326)
ggplot(sp_wgs84) + geom_sf()
sf::st_crs(sp_wgs84)
sp_geo <- sf_geojson(sp_wgs84, simplify = TRUE, digits = 6)
write_file(sp_geo, 'dados2.json')
# dados daqui:
# http://dados.prefeitura.sp.gov.br/pt_PT/dataset/distritos/resource/9e75c2f7-5729-4398-8a83-b4640f072b5d
sp <- sf::read_sf('LAYER_DISTRITO')
simple_sp <- rmapshaper::ms_simplify(sp)
sp_wgs84 <- sf::st_transform(simple_sp, 4326)
ggplot(sp_wgs84) + geom_sf()
sp_geo <- geojsonsf::sf_geojson(sp_wgs84, simplify = TRUE, digits = 6)
